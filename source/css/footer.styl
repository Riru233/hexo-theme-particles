    text-align left
    top 300px
    background body-background
    padding-bottom 35px

    .footer-wrap
        width 900px
        margin auto
        color label-font

        div
            margin 15px

        a
            text-decoration none
            color default-link

    .footer-icon
        display inline-block
        margin 0 10px
        color default-link
        font-size: 18px